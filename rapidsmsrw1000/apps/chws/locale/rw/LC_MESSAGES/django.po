#: handlers/who.py:50
msgid "You need to be registered first, use the REG keyword"
msgstr "Ugomba kubanza kwiyandikisha, koresha ijambo REG"

#: handlers/last.py:59
msgid "You have not yet sent a report."
msgstr "Nta raporo yanyu duherutse kubona."

#: handlers/last.py:68
#, python-format
msgid " Date: %(date)s"
msgstr "Itariki: %(date)s"

#: handlers/reg.py:50
msgid "The correct message format is: SUP YOUR_ID CLINIC_ID LANG VILLAGE"
msgstr ""
"wagombye kwandika: SUP NUMERO_Y_INDANGAMUNTU NUMERO_Y_IBITARO URURIMI_UVUGA "
"UMUDUGUDU"

#: handlers/reg.py:52
msgid "The correct message format is: REG YOUR_ID CLINIC_ID LANG VILLAGE"
msgstr ""
"wagombye kwandika: REG NUMERO_Y_INDANGAMUNTU NUMERO_Y_IBITARO URURIMI_UVUGA "
"UMUDUGUDU"

#: handlers/reg.py:58
#, python-format
msgid ""
"Error.  National ID must be exactly 16 digits, you sent the id: %(nat_id)s"
msgstr ""
"Ikosa. Nomero y'irangamuntu igomba kugira imibare 16, wohereje %(nat_id)s"

#: handlers/reg.py:77
msgid "Please contact your Health Centre to register your National ID"
msgstr "Banza ujye kwiyandikisha ku kigo nderabuzima mu gitabo cy'abajyanama."

#: handlers/reg.py:98
#, python-format
msgid "Unknown Health Clinic ID: %(clinic)s"
msgstr "Iyi nimero y'ibitaro ntizwi: %(clinic)s"

#: handlers/reg.py:136
#, python-format
msgid "Thank you for registering at %(clinic)s"
msgstr "Murakoze kwiyandikisha kuri iki kigo nderabuzima %(clinic)s"

#: handlers/who.py:55
#, python-format
msgid "You are not in a group, located at %(location)s, you speak %(language)s"
msgstr ""
"Ntabwo uriyandikisha neza, uherereye i %(location)s, uvuga %(language)s"

#: handlers/who.py:62
#, python-format
msgid "You are a %(group)s, located at %(location)s, you speak %(language)s"
msgstr "Uri %(group)s, uherereye i %(location)s, uvuga %(language)s"

#: handlers/who.py:66
msgid "We don't recognize you"
msgstr ""
"Ntabwo tukuzi muri RapidSMS, banza ujye kwiyandikisha ku kigo nderabuzima "
"cyawe."
